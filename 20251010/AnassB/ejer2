import java.util.Scanner;

public class del1a200pares {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);


        int numero;
        int contador = -2;

        while (contador<200) {
            contador = contador +2;
            System.out.println(contador);
        }
        System.out.println("Estos son los numeros pares entre el 1 y el 200");

    }

}

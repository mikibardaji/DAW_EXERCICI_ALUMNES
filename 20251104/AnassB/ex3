import java.util.Scanner;

public class ejercicio3funcionales {
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);

System.out.print("Muestra un  umero ");
int numero1 =  sc.nextInt();
System.out.print("Muestra otro ");
int numero2 =  sc.nextInt();

System.out.println("El menor entre el a y el b es el: " + minimo(numero1, numero2));

}

public static int minimo(int a, int b) {
    if (a<b) {
        return a;
    } else return b;
}



}
